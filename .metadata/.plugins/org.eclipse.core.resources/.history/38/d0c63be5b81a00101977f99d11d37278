package com.shyam;

public class ReverseArrayInPlace {
public static void main(String[] args) {
	int[] arr= {12,3,5,6,7,8};
}

public static void reverseArr(int[] arr) {
	int left=0;
	int right=arr.length-1;
	
	while(left<right) {
		int temp=arr[left];
		arr[left]=arr[right];
		arr[right]=temp;
		
		left++;
		right--;
	}
}
}
